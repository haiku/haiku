if test -f ../../../../../generated/tests/libbe_test/x86/apps/BitmapBounds; then
	../../../../../generated/tests/libbe_test/x86/apps/BitmapBounds
else
	echo "You need to \"TARGET_PLATFORM=libbe_test jam install-test-apps\" first."
fi
