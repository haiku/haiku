SubDir HAIKU_TOP src apps diskprobe ;

SetSubDirSupportedPlatformsBeOSCompatible ;

UsePrivateHeaders shared ;

Application DiskProbe :
	DiskProbe.cpp
	DataEditor.cpp
	DataView.cpp
	ProbeWindow.cpp
	FileWindow.cpp
	AttributeWindow.cpp
	TypeEditors.cpp
	ProbeView.cpp
	OpenWindow.cpp
	FindWindow.cpp
	: be $(HAIKU_LOCALE_LIBS) tracker translation libexpression_parser.a
		libmapm.a $(TARGET_LIBSUPC++)
	: DiskProbe.rdef
;

DoCatalogs DiskProbe :
	x-vnd.Haiku-DiskProbe
	:
	AttributeWindow.cpp
	DiskProbe.cpp
	FindWindow.cpp
	FileWindow.cpp
	OpenWindow.cpp
	ProbeView.cpp
	TypeEditors.cpp
;
