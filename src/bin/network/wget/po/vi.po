#, c-format
msgid "Using `%s' as listing tmp file.\n"
msgstr "Đang dùng `%s' làm tập tin danh sách tmp.\n"

#: src/ftp.c:1339
#, c-format
msgid "Removed `%s'.\n"
msgstr "Đã xóa `%s'.\n"

