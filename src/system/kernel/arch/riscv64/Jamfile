        arch_system_info.cpp
        arch_timer.cpp
        arch_thread.cpp
        arch_user_debugger.cpp
        arch_vm.cpp
        arch_vm_translation_map.cpp
        RISCV64VMTranslationMap.cpp
        Htif.cpp
        sbi_syscalls.S
        :
        $(TARGET_KERNEL_PIC_CCFLAGS) -Wno-unused
        :
;
