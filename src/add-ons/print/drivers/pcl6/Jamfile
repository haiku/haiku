SubDir HAIKU_TOP src add-ons print drivers pcl6 ;

SetSubDirSupportedPlatformsBeOSCompatible ;

SubDirHdrs [ FDirName $(HAIKU_TOP) headers private print libprint ] ; 
SubDirHdrs [ FDirName $(HAIKU_TOP) headers private print utils ] ; 

AddResources PCL6\ Compatible : PCL6.rsrc ;

Addon PCL6\ Compatible :
	DeltaRowCompression.cpp
	PCL6Entry.cpp
	PCL6.cpp
	PCL6Cap.cpp
	PCL6Rasterizer.cpp
	PCL6Writer.cpp
	Rasterizer.cpp
	:
	be 
	libprint.a 
	libprintutils.a
	$(TARGET_LIBSTDC++)
;

Package haiku-printingkit-cvs :
	PCL6\ Compatible :
	boot home config add-ons Print ;
